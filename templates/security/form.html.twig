{% extends 'base.html.twig' %}
{% block body %}
<header><center><h1>Registration!</h1></center></header>
<div class="container">
    <div class="form">
    <center>
    {{ form_start(form) }}
        {{ form_row(form.username) }}
        {{ form_row(form.email) }}
        {{ form_row(form.plainPassword.first) }}
        {{ form_row(form.plainPassword.second) }}
        <button type="submit">Register!</button>
        {{ form_end(form) }}
    </center>
    </div>
</div>

{% endblock %}

